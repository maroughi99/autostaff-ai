{
  "name": "workbot-ai",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:api": "cd apps/api && npm run build",
    "start:api": "cd apps/api && npm run start:prod",
    "migrate": "cd packages/database && npx prisma migrate deploy",
    "lint": "turbo run lint",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "^3.1.0",
    "turbo": "^1.11.2",
    "typescript": "^5.3.3"
  },
  "packageManager": "npm@10.2.3"
}
